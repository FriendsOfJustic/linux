<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rsgisai.geohazard.level.dao.pgsql.LandslideActivityModelDao">
    <insert id="insert" useGeneratedKeys="true" keyProperty="id">
        insert into landslide_activity_model(model)
        values (#{serializedObject})
    </insert>
    <select id="selectLast"
            resultType="com.rsgisai.geohazard.system.common.entity.LinearModel">
        select id, model as serializedObject
        from landslide_activity_model
        order by id desc
        limit 1
    </select>
</mapper>
