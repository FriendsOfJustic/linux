<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rsgisai.geohazard.computation.geospatial.dao.pgsql.OriginRasterDao">

    <insert id="upload">
        insert into public.origin_raster(working_area_id,
                                         feature,
                                         rast,
                                         create_by,
                                         modify_by)
        values (#{workingAreaId},
                #{feature},
                #{wkbHex}::raster,
                #{userId},
                #{userId}
                );
    </insert>
</mapper>
